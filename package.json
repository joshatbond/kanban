{
  "name": "kanception",
  "private": true,
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -r npm:dev:web npm:dev:db",
    "dev:web": "vinxi dev",
    "dev:db": "convex dev",
    "db:seed": "convex dev --run board:seed",
    "build": "vinxi build 2>/dev/null & true 'Ignore vinxi build warnings until tanstack start can run without them (need updated guide)'",
    "start": "vinxi start",
    "lint": "prettier --check '**/*' --ignore-unknown && eslint --ext .ts,.tsx ./app",
    "format": "prettier --write '**/*' --ignore-unknown"
  },
  "dependencies": {
    "@clerk/tanstack-start": "0.6.5",
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@radix-ui/react-slot": "^1.1.2",
    "@t3-oss/env-core": "^0.12.0",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-router": "^1.92.11",
    "@tanstack/react-router-with-query": "^1.92.11",
    "@tanstack/react-start": "^1.112.11",
    "@tanstack/router-devtools": "^1.92.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "1.17.4",
    "lucide-react": "^0.479.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "redaxios": "^0.5.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "0.5.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.32.0",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/eslint-plugin-router": "^1.114.12",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "babel-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "concurrently": "~8.2.2",
    "eslint-plugin-react-compiler": "^19.0.0-beta-bafa41b-20250307",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite-tsconfig-paths": "^5.1.3"
